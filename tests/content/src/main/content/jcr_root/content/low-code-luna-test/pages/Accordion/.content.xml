<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
  xmlns:ws="http://ds.pl/websight" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
  jcr:primaryType="ws:Page">
  <jcr:content
    jcr:primaryType="ws:PageContent"
    jcr:title="Accordion"
    sling:resourceType="luna/components/page"
    ws:template="/apps/luna/templates/basicpage">
    <pagecontainer
      jcr:primaryType="nt:unstructured"
      sling:resourceType="kyanite/common/components/pagecontainer">
      <accordion
        jcr:primaryType="nt:unstructured"
        sling:resourceType="luna/components/accordion">
        <accordionitem
          jcr:primaryType="nt:unstructured"
          sling:resourceType="luna/components/accordion/accordionitem"
          header="Some awesome header"/>
        <accordionitem_2
          jcr:primaryType="nt:unstructured"
          sling:resourceType="luna/components/accordion/accordionitem"
          header="Some awesome header 2"/>
      </accordion>
    </pagecontainer>
  </jcr:content>
</jcr:root>
